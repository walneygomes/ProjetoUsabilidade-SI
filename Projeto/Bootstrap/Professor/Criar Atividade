										<div class="tric">
										Aula: <input type="text" name="fname" id="fname" /><br/><br/>
										Data: <input type="text" name="lname" id="lname" /><br/><br/>
										Inicio: <input type="text" name="age" id="age" /><br/><br/>
										Termino: <input type="text" name="termino" id="termino" /><br/><br/>
										Conteudo Previsto: <input type="text" name="conteudo" id="conteudo" /><br/><br/>
										Conteudo Realizado: <input type="text" name="realizado" id="realizado" /><br/><br/>
										Conteudo tipo: <input type="text" name="tipo" id="tipo" /><br/><br/>

				
</div>				<button onclick="addRow();">Add Row</button><br/><br/>
				<script>
            
            function addRow()
            {
                // get input values
                var fname = document.getElementById('fname').value;
                 var lname = document.getElementById('lname').value;
                  var age = document.getElementById('age').value;
                    var termino = document.getElementById('termino').value;
                    var conteudo = document.getElementById('conteudo').value;
						var realizado = document.getElementById('realizado').value;
                  						var tipo = document.getElementById('tipo').value;


                  // get the html table
                  // 0 = the first table
                  var table = document.getElementsByTagName('table')[0];
                  
                  // add new empty row to the table
                  // 0 = in the top 
                  // table.rows.length = the end
                  // table.rows.length/2+1 = the center
                  var newRow = table.insertRow(table.rows.length);
                  
                  // add cells to the row
                  var cel1 = newRow.insertCell(0);
                  var cel2 = newRow.insertCell(1);
                  var cel3 = newRow.insertCell(2);
				  var cel4 = newRow.insertCell(3);
				  var cel5 = newRow.insertCell(4);
				  var cel6 = newRow.insertCell(5);
				  var cel7 = newRow.insertCell(6);

                  
                  // add values to the cells
                  cel1.innerHTML = fname;
                  cel2.innerHTML = lname;
                  cel3.innerHTML = age;
				 cel4.innerHTML = termino;
				 cel5.innerHTML = conteudo;
				 cel6.innerHTML = realizado;
				 cel7.innerHTML = realizado;

            }
            
        </script>


       
										</div>
										</div>
